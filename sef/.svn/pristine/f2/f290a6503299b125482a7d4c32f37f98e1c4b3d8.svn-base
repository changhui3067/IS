package com.successfactors.sef.service;

import java.util.List;

import com.successfactors.sca.ServiceQuery;
import com.successfactors.sef.bean.SEFSubscriberConfigurationVO;

/**
 * GetEventMetaData
 * 
 */
public class GetSubscriberConfigurationData implements
    ServiceQuery<List<SEFSubscriberConfigurationVO>> {

  private String eventType;

  private String eventTopic;

  private String categoryID;

  private String subscriberID;
  
  private Boolean singleQuery;

  /**
   * @return the eventType
   */
  public String getEventType() {
    return eventType;
  }

  /**
   * @param eventType
   *          the eventType to set
   */
  public void setEventType(final String eventType) {
    this.eventType = eventType;
  }

  /**
   * @return the eventTopic
   */
  public String getEventTopic() {
    return eventTopic;
  }

  /**
   * @param eventTopic
   *          the eventTopic to set
   */
  public void setEventTopic(final String eventTopic) {
    this.eventTopic = eventTopic;
  }

  /**
   * @return the categoryID
   */
  public String getCategoryID() {
    return categoryID;
  }

  /**
   * @param categoryID
   *          the categoryID to set
   */
  public void setCategoryID(final String categoryID) {
    this.categoryID = categoryID;
  }

  /**
   * @return the subscriberID
   */
  public String getSubscriberID() {
    return subscriberID;
  }

  /**
   * @param subscriberID
   *          the subscriberID to set
   */
  public void setSubscriberID(final String subscriberID) {
    this.subscriberID = subscriberID;
  }


  /**
   * @return the singleQuery
   */
  public Boolean getSingleQuery() {
    return singleQuery;
  }

  /**
   * @param singleQuery the singleQuery to set
   */
  public void setSingleQuery(Boolean singleQuery) {
    this.singleQuery = singleQuery;
  }

  /**
   * Constructor
   */
  public GetSubscriberConfigurationData() {

  }

}
