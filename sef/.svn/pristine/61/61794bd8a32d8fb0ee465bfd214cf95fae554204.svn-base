<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ivy-module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="2.0" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info branch="trunk" module="au-sef-ui" organisation="com.successfactors" />

	<configurations>
		<conf name="compile" description="Compile Dependencies"
			visibility="public" />
		<conf name="test" description="Test compile Dependencies"
			visibility="public" />
		<conf name="runtime" description="Test runtime Dependencies"
			visibility="public" />
		<conf name="webapp-content" description="Web Dependencies to add to war"
			visibility="public" />
		<conf name="default" description="Default artifact" visibility="public" />
		<conf name="pom" description="Maven pom file" visibility="public" />
		<conf name="src" description="Source artifact" visibility="public" />
	</configurations>

	<publications>
		<artifact conf="default" name="au-sef-ui" />
		<artifact conf="src" name="au-sef-ui-source" ext="jar" type="source" />
		<artifact conf="pom" name="au-sef-ui" ext="pom" />
	</publications>

	<dependencies>
		<dependency conf="compile->default" name="jboss-seam-custom"
			org="org.jboss" rev="${org.jboss.jboss-seam-custom.version}" />
		<dependency branch="trunk" conf="compile->default" name="au-execmanager-service"
			org="com.successfactors" rev="${com.successfactors.au-execmanager.version}" />
		<dependency branch="trunk" conf="compile->default" name="au-sef-service"
			org="com.successfactors" rev="${com.successfactors.au-sef.version}" />
		<dependency branch="trunk" conf="compile->default" name="au-V4-service"
			org="com.successfactors" rev="${com.successfactors.au-V4.version}" />
		<dependency branch="trunk" conf="compile->default" name="au-V4-ui"
			org="com.successfactors" rev="${com.successfactors.au-V4.version}" />
		<dependency branch="trunk" conf="compile->default" name="idl-ajaxservice-service"
			org="com.successfactors" rev="${com.successfactors.idl-ajaxservice.version}" />
		<dependency branch="trunk" conf="compile->default" name="idl-i18n-service"
			org="com.successfactors" rev="${com.successfactors.idl-i18n.version}" />
		<dependency branch="trunk" conf="compile->default" name="idl-logging-api"
			org="com.successfactors" rev="${com.successfactors.idl-logging.version}" />
		<dependency branch="trunk" conf="compile->default" name="idl-sca-service"
			org="com.successfactors" rev="${com.successfactors.idl-sca.version}" />
		<dependency branch="trunk" conf="compile->default" name="idl-smrf-service"
		    org="com.successfactors" rev="${com.successfactors.idl-smrf.version}" />
        <dependency branch="trunk" conf="compile->default" name="javax.inject" org="javax.inject" rev="${javax.inject.javax.inject.version}"/>
        <dependency branch="trunk" conf="compile->default" name="idl-platform-service" org="com.successfactors" rev="${com.successfactors.idl-platform.version}"/>
        <dependency branch="trunk" conf="compile->default" name="spring-context" org="org.springframework" rev="${org.springframework.spring-context.version}"/>
	</dependencies>

</ivy-module>
